        android:id="@+id/campoMateria1"
        android:layout_width="214dp"
        android:layout_height="42dp"
        android:ems="10"
        android:hint="Digite Materia1"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
